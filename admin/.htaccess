RewriteEngine On    # Turn on the rewriting engine
RewriteRule     ^login/?$       index.php     [NC,L]
RewriteRule     ^reset/?$       resetpassword.php	[NC,L]
RewriteRule     ^logout/?$      logout.php    [NC,L]
RewriteRule     ^start/?$       index.php     [NC,L]
RewriteRule     ^upload\ coverimages/?$   upload_cover.php   [NC,L]
RewriteRule     ^upload\ images/?$   upload_images.php   [NC,L]
RewriteRule     ^change\ brandnames/?$   change_brands.php   [NC,L]
